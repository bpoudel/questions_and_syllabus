                    "item": {
                        "itemData": {
                            "question": {
                                "content": "**Apply the distributive property to factor out the greatest common factor.**  $9+15=$",
                                "images": [],
                                "options": [
                                    {
                                        "content": "3(3+5)",
                                        "correct": false
                                    }
                                ]
                            },
                            "hints": [
                                {
                                    "content": "$\\blue{3}$ is the greatest common factor of ${9}$ and ${15}$.",
                                    "images": []
                                },
                                {
                                    "content": "$\\phantom{=}{9} + {15}$  $=\\blue{3}\\cdot3+\\blue{3}\\cdot5$",
                                    "images": []
                                },
                                {
                                    "content": "$=\\blue{3}(3+5) ~~~~~~~~$ Apply the distributive property.",
                                    "images": []
                                },
                                {
                                    "content": "The answer:  > $ {3}(3+5)$",
                                    "images": []
                                }
                            ]
                        },
                        "problemType": "Easy"
                    }
                }
            }
        },
