                        "itemData": {
                            "question": {
                                "content": "**What is the slope of the line through $(1,0)$ and $(3,8)$?**",
                                "images": [],
                                "options": [
                                    {
                                        "content": "$4$",
                                        "correct": true,
                                        "images": []
                                    },
                                    {
                                        "content": "$\\dfrac14$",
                                        "correct": false,
                                        "images": []
                                    },
                                    {
                                        "content": "$-4$",
                                        "correct": false,
                                        "images": []
                                    },
                                    {
                                        "content": "$-\\dfrac14$",
                                        "correct": false,
                                        "images": []
                                    }
                                ]
                            },
                            "hints": [
                                {
                                    "content": "$\\text{Slope}=\\dfrac{\\text{Rise}}{\\text{Run}}=\\dfrac{\\text{Change in }y}{\\text{Change in }x}$",
                                    "images": []
                                },
                                {
                                    "content": "$\\begin{align} \\text{Slope}&=\\dfrac{8-0}{3-1} \\\\\\\\ &=\\dfrac{8}{2} \\\\\\\\ &=4 \\end{align}$",
                                    "images": []
                                },
                                {
                                    "content": "The slope is $4$.",
                                    "images": []
                                }
                            ]
                        },
                        "problemType": "1. Slope is positive integer"
                    }
                }
            }
        },
