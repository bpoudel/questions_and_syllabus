                        "itemData": {
                            "question": {
                                "content": "** What is $84\\%$ of $300$? **",
                                "images": [],
                                "options": [
                                    {
                                        "content": "252",
                                        "correct": true
                                    }
                                ]
                            },
                            "hints": [
                                {
                                    "content": "To find $84\\%$ of $300$, we can multiply $84\\%$ by $300$.  How do we multiply by a percent?",
                                    "images": []
                                },
                                {
                                    "content": "Percent means per-hundred.  $84\\%=84\\text{ per hundred} =\\dfrac{84}{100}$",
                                    "images": []
                                },
                                {
                                    "content": "$\\begin{align}  84\\%\\times 300&=\\dfrac{84}{100}\\times300\\\\ \\\\ &=\\dfrac{84\\times 300}{100} \\\\ \\\\ &= \\dfrac{ 84 ~~\\times \\stackrel{3}{\\cancel{300}}}{  \\underset{1}{\\cancel{100}}}  \\\\ \\\\ &=\\dfrac{84\\times3}1\\\\ \\\\ &=252 \\end{align}$",
                                    "images": []
                                },
                                {
                                    "content": "$84\\%$ of $300$ is $252$.",
                                    "images": []
                                }
                            ]
                        },
                        "problemType": "find part "
                    }
                }
            }
        },
