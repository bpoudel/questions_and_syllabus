                    "item": {
                        "itemData": {
                            "question": {
                                "content": "**Select the equivalent expression.**  $\\dfrac{1}{2^8}=?$",
                                "images": [],
                                "options": [
                                    {
                                        "content": "$2^{-8}$",
                                        "correct": true,
                                        "images": []
                                    },
                                    {
                                        "content": "$2^{^{\\frac18}}$",
                                        "correct": false,
                                        "images": []
                                    },
                                    {
                                        "content": "$\\dfrac{1^8}{2}$",
                                        "correct": false,
                                        "images": []
                                    }
                                ]
                            },
                            "hints": [
                                {
                                    "content": "This is the definition for negative exponents:  $x^{-n}=\\dfrac{1}{x^n}$",
                                    "images": []
                                },
                                {
                                    "content": "Applying the definition in the reverse direction, we get:  $\\dfrac{1}{2^8}=2^{-8}$.",
                                    "images": []
                                }
                            ]
                        },
                        "problemType": "1. Numerical pos->neg"
                    }
                }
            }
        },
