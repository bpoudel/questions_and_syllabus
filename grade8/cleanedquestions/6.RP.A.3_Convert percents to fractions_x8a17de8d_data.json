                        "itemData": {
                            "question": {
                                "content": "The audience  thinks $30\\%$ of Brandon's jokes are funny.  **What fraction of Brandon's jokes does the audience think are funny?**",
                                "images": [],
                                "options": [
                                    {
                                        "content": "0.3",
                                        "correct": true
                                    }
                                ]
                            },
                            "hints": [
                                {
                                    "content": "A percent is a fraction out of $100$.",
                                    "images": []
                                },
                                {
                                    "content": "$\\purpleD{30}\\%=\\dfrac\\purpleD{30}{100}$",
                                    "images": []
                                },
                                {
                                    "content": "Let's simplify the fraction:  $\\begin{align}&\\dfrac{\\purpleD{30}}{100}\\\\\\\\ =&\\dfrac{\\purpleD{30}}{100}\\div\\dfrac{10}{10}\\end{align}$   [[☃ explanation 1]]",
                                    "images": []
                                },
                                {
                                    "content": "The audience thinks $\\dfrac{3}{10}$ of Brandon's jokes is funny.",
                                    "images": []
                                }
                            ]
                        },
                        "problemType": "context"
                    }
                }
            }
