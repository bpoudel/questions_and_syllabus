                    "item": {
                        "itemData": {
                            "question": {
                                "content": "**Express $0.02$ as a percent.**     $\\%$",
                                "images": [],
                                "options": [
                                    {
                                        "content": "2",
                                        "correct": true
                                    }
                                ]
                            },
                            "hints": [
                                {
                                    "content": "Rewrite the decimal as a fraction with a denominator of 100.   $0.02 = \\dfrac{2}{100}$",
                                    "images": []
                                },
                                {
                                    "content": "$\\hphantom{0.02} = 2\\%$   $2$ per hundred = $2$ per cent = $2$ percent",
                                    "images": []
                                },
                                {
                                    "content": "Another way to convert a decimal to a percent is to multiply by $100$ (or move the decimal two places to the right) and add a percent sign.   $0.02 \\times 100= {2}\\%$",
                                    "images": []
                                }
                            ]
                        },
                        "problemType": "< 100"
                    }
                }
            }
