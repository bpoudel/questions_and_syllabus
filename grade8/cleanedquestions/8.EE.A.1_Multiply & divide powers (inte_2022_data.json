                        "itemData": {
                            "question": {
                                "content": "**Simplify.**   *Rewrite the expression in the form $4^n$.*  $(4^{11})(4^{-8})=$",
                                "images": [],
                                "options": [
                                    {
                                        "content": "4^3",
                                        "correct": false
                                    }
                                ]
                            },
                            "hints": [
                                {
                                    "content": "Recall that $x^n\\cdot x^m=x^{n+m}$.",
                                    "images": []
                                },
                                {
                                    "content": "$\\begin{align} (4^{11})(4^{-8})&=4^{11+(-8)} \\\\\\\\ &= 4^{11-8} \\\\\\\\ &= 4^{3} \\end{align}$",
                                    "images": []
                                }
                            ]
                        },
                        "problemType": "1. Product (numerical)"
                    }
                }
            }
