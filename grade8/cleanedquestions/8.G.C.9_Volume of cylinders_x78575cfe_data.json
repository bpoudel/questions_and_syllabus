                        "itemData": {
                            "question": {
                                "content": "**Find the volume of the cylinder.**   *Either enter an exact answer in terms of $\\pi$ or use $3.14$ for $\\pi$.*      units$^3$",
                                "images": [
                                    {
                                        "alt": "A cylinder with a base radius of 2 units and a height of 5 units.",
                                        "url": "https://cdn.kastatic.org/ka-perseus-graphie/b3c982d3f07091beba0fca109a868870e8e6826a.svg"
                                    }
                                ],
                                "options": [
                                    {
                                        "content": "20\\pi",
                                        "correct": false
                                    },
                                    {
                                        "content": "62.8",
                                        "correct": false
                                    }
                                ]
                            },
                            "hints": [
                                {
                                    "content": "$\\text{Volume of a cylinder} = \\pi r^2 h$",
                                    "images": []
                                },
                                {
                                    "content": "$\\begin{align} \\phantom{\\text{Volume of a sphere}} &= \\pi \\cdot2^2 \\cdot 5 \\\\\\\\ &= \\pi \\cdot 4 \\cdot 5 \\\\\\\\ &= 20 \\pi\\\\\\\\ \\end{align}$",
                                    "images": []
                                },
                                {
                                    "content": "The volume of the cylinder is $20\\pi$ units$^3$.  *(Note that we could also multiply $20$ by $3.14$ to get $62.8$ units$^3$.)*",
                                    "images": []
                                }
                            ]
                        },
                        "problemType": "Type 3 (8 items)"
                    }
                }
            }
        },
