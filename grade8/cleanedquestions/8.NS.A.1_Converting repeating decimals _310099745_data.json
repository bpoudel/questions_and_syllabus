                                "options": [
                                    {
                                        "content": "0.1111111111111111",
                                        "correct": true
                                    }
                                ]
                            },
                            "hints": [
                                {
                                    "content": "Let $x$ equal the decimal. Set up two equations such that the digits after the decimal point are identical.",
                                    "images": []
                                },
                                {
                                    "content": "$\\large{\\begin{align*} 10x &= 1.1111...\\\\ x &= 0.1111... \\end{align*}} $",
                                    "images": []
                                },
                                {
                                    "content": "Subtracting the two equations, we have:   $\\large{9x = 1}$",
                                    "images": []
                                },
                                {
                                    "content": "$\\large{ x = \\dfrac{1}{9}} $",
                                    "images": []
                                }
                            ]
                        },
                        "problemType": "problem-type-0"
