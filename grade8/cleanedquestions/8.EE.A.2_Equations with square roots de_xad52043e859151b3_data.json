                    "item": {
                        "itemData": {
                            "question": {
                                "content": "**Find the solution(s) of the following equation.**  $v^2 = \\dfrac{25}{81}$",
                                "images": [],
                                "options": [
                                    {
                                        "content": "$v = \\dfrac{5}{81}$",
                                        "correct": false,
                                        "images": []
                                    },
                                    {
                                        "content": "$v = -\\dfrac{5}{81}$",
                                        "correct": false,
                                        "images": []
                                    },
                                    {
                                        "content": "$v = \\sqrt{\\dfrac{25}{81}}$",
                                        "correct": true,
                                        "images": []
                                    },
                                    {
                                        "content": "$v = -\\sqrt{\\dfrac{25}{81}}$",
                                        "correct": true,
                                        "images": []
                                    },
                                    {
                                        "content": "",
                                        "correct": false,
                                        "images": []
                                    }
                                ]
                            },
                            "hints": [
                                {
                                    "content": "Let's solve for $v$.",
                                    "images": []
                                },
                                {
                                    "content": "$\\begin{align} v^2 &= \\dfrac{25}{81}\\\\ \\\\ \\sqrt{v^2} &= \\sqrt{\\dfrac{25}{81}}\\\\ \\\\ v &=\\pm {\\dfrac{\\sqrt{25}}{\\sqrt{81}}}\\\\ \\\\  v&= \\pm\\dfrac59 \\end{align}$",
                                    "images": []
                                },
                                {
                                    "content": "$v = \\sqrt{\\dfrac{25}{81}}$ or $v = -\\sqrt{\\dfrac{25}{81}}$",
                                    "images": []
                                }
                            ]
                        },
                        "problemType": "1. Solve x^2=k where k is a fraction"
                    }
                }
            }
        },
